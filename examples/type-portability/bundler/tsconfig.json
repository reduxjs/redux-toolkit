{
  "extends": "@reduxjs/tsconfig/bundler/esnext",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "emitDeclarationOnly": false,
    "jsx": "react-jsx",
    "noEmit": false,
    "noEmitOnError": true,
    "outDir": "./dist",
    "verbatimModuleSyntax": true
  },
  "include": ["src"]
}
